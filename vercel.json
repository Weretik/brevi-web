{
  "version": 2,
  "functions": {
    "api/ssr.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/dist/apps/storefront/browser/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/api/ssr.js"
    }
  ]
}
